<ol class="navigation-explore" data-panel="explore">
  {{#each site.domains}}
  <li class="flex strong justify-justified domain{{#if ./selected}} domain--current{{/if}}">
    <a class="strong domain-title">{{{./title}}}</a>
      {{#unless (eq this.versions.length 1)}}
      <select class="select version-select js-version">
        <option disabled>Select version
        {{#each this.versions}}
        <option {{#if ./selected}}selected{{/if}} value="{{{./url}}}">{{./string}}{{#if ./latest}} (latest){{/if}}
        {{/each}}
      </select>
      {{/unless}}
  {{/each}}
</ol>
{{!-- {{> navigation-tree }} --}}
